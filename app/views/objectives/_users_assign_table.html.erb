<table class="table table-striped">
  <thead>
  <tr>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  	<% users.each do |user| %>
  		<tr>
			  <td><%= user.email %></td>
			  <td><%= user.first_name %></td>
			  <td><%= user.last_name%></td>
			  <td><%#= link_to goal_path(id: goal.id), data: { confirm: 'Are you sure?' }, method: :delete do %><i class="fa fa-remove"></i> 
			  <%#end%> </td>
			</tr>
  	<%end%>
  </tbody>
</table>