<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
      <h2><%= current_user.organization_name%></h2>
      <ol class="breadcrumb">
          <li>
              <a href="<%= url_for :controller => 'dashboards', :action => 'dashboard_1' %>">Home</a>
      </li>
      <li>
          <%= link_to "Organization Setup", organization_divisions_path(organization_id: params[:id]) %>
      </li>
      <li class="active">
          <strong>
          	<%= link_to "Edit", edit_organization_path(organization_id: params[:id])%>
          </strong>
      </li>
  </ol>
  </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
<div class="row">
<div class="col-lg-1">
</div>
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Edit Organization</h5>
                <div class="ibox-tools">
                	  <%= link_to organization_divisions_path(organization_id: params[:id]),style: "color:grey;" do %><i class="fa fa-remove"></i> 
                    <%end%>
                </div>
            </div>
            <div class="ibox-content">
                <%= render 'form' %>
            </div>
        </div>
    </div>
</div>
</div>